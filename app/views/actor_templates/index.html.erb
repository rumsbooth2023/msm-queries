<h1> List of all actors </h1>

<table border="1">
<th> ID </th>
<th> Image </th>
<th> Name </th>
<th> DOB </th>
<th> Bio </th>
<%= Actor.all.each do |a_thing| %>
<tr>
  <td><%= a_thing.id%></td>
  <td><img src = "<%= a_thing.image%>"></td>
  <td><%= a_thing.name%></td>
  <td><%= a_thing.dob%></td>
  <td>
    <a href = "/actors/<%= a_thing.id%>">
    Show Details
    </a>
  </td>
</tr
<% end %>
</table>
